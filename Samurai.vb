    Public Function intSamuraiOffset(ByVal intCell As Integer, ByVal IntGrid As Integer) As Integer
        Dim intRow As Integer
        Dim intCol As Integer
        intRow = intFindRow(intCell)
        intCol = intFindCol(intCell)
        Select Case IntGrid
            Case 1
                intSamuraiOffset = intCol + (intRow - 1) * 21
            Case 2
                intSamuraiOffset = ((intRow - 1) * 21) + 12 + intCol
            Case 3
                intSamuraiOffset = (intRow + 5) * 21 + (intCol + 6)
            Case 4
                intSamuraiOffset = intCol + (intRow + 11) * 21
            Case 5
                intSamuraiOffset = 12 + intCol + (intRow + 11) * 21
        End Select
    End Function
